<div class="section no-pad-bot" id="index-banner">
  <div class="container">

    <div class="the_crumbs">
      <div class="col s12">
        <a href="/" class="breadcrumb">Home</a>
        <i class="fa fa-chevron-right" aria-hidden="true"></i>
        <a href="/category" class="breadcrumb">Category</a>
      </div>
    </div>


    <div id="category-flashcards">
      {{#each cats}}
          {{#if this.cat_name}}
              <h5>{{ this.cat_name }} - Sets</h5>
          {{/if}}
      {{/each}}


      <div class="row" >
      <!-- allCategories.cats[0].Sets[0].title -->

        {{#each cats}}
          {{#if this.cat_name}}
            {{#each this.Sets}}
          <div class="col s12 set"><a href="#play-flashcards" value="view-set" class="btn modal-trigger card-panel" >{{ title }}</a></div>
          <p>Created by <a href="#">(User Name)</a></p>
            {{/each}}
          {{/if}}
        {{/each}}

      </div>
    </div> <!-- end category Flashcards -->

  </div>
</div>

